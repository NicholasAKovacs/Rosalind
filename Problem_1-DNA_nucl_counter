f = open('rosalind_dna.txt', 'r')
G = 0
C = 0
A = 0
T = 0
for x in f:
    x.split()
    for y in x:
        if y == 'G':
            G+=1
        if y == 'C':
            C+=1
        if y == 'A':
            A+=1
        if y == 'T':
            T+=1
print 'A: ',A
print 'C: ',C
print 'G: ',G
print 'T: ',T

f.close()
